{
  "compilerOptions": {
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@hooks/*": [
        "./hooks/*"
      ],
      "@utils/*": [
        "./utils/*"
      ],
      "@contexts/*": [
        "./contexts/*"
      ],
      "@providers/*": [
        "./providers/*"
      ],
      "@components/*": [
        "./components/*"
      ],
      "@services/*": [
        "./services/*"
      ]
    },
    "target": "ES2017"
  },
  "include": [
    "app/types",
    "src",
    "**/*.ts",
    "**/*.tsx",
    "next-env.d.ts",
    ".prettierrc.mjs",
    "eslint.config.mjs",
    ".next/types/**/*.ts",
    "build/types/**/*.ts",
    "app/members/signout/metadata.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
